<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.visitorapp.model.Customer" />
	<import class="com.visitorapp.model.CustomerStoreVisit" />
	<import class="com.visitorapp.model.StoreLocation" />
	<class name="com.visitorapp.model.impl.CustomerImpl" table="VisitorApp_Customer">
		<cache usage="read-write" />
		<id name="customerId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="picture" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.visitorapp.model.impl.CustomerStoreVisitImpl" table="VisitorApp_CustomerStoreVisit">
		<cache usage="read-write" />
		<id name="customerstorevisitId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="customerId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="storelocationId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.visitorapp.model.impl.StoreLocationImpl" table="VisitorApp_StoreLocation">
		<cache usage="read-write" />
		<id name="storelocationId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>